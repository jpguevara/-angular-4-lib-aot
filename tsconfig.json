{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./.aot/src/",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "files": [
    "./.aot/src/index.ts",
    "./.aot/src/module-a/mi/mi.module.ts"
  ],
  "types": [
    "node",
    "jasmine"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": false,
    "debug": true,
    "entryModule": "index#ModuleAModule",
    "skipMetadataEmit": false,
    "strictMetadataEmit": true
  }
}